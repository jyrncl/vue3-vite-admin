{
    "tabWidth": 2,
    "semi": true,
    "trailingComma":  "none",
    "printWidth": 1000,
    "arrowParens": "avoid",
    "bracketSpacing": true,

    "endOfLine": "auto",
    "useTabs": false,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "jsxBracketSameLine": false,
    "rangeStart": 0,
    "requirePragma": false,
    "insertPragma": false,
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css"
}
